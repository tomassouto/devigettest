<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-right" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Cms\Block\Block" name="homeblock1">
                <arguments>
                    <argument name="block_id" xsi:type="string">home_cms_block_1</argument>
                </arguments>
            </block>
            <block class="Magento\Cms\Block\Block" name="homeblock2">
                <arguments>
                    <argument name="block_id" xsi:type="string">home_cms_block_2</argument>
                </arguments>
            </block>
            <block class="Magento\Cms\Block\Block" name="homeblock3">
                <arguments>
                    <argument name="block_id" xsi:type="string">home_cms_block_3</argument>
                </arguments>
            </block>
            <block class="Magento\Cms\Block\Block" name="homeblock4">
                <arguments>
                    <argument name="block_id" xsi:type="string">home_cms_block_4</argument>
                </arguments>
            </block>
            <block class="Magento\Cms\Block\Block" name="homeblock5">
                <arguments>
                    <argument name="block_id" xsi:type="string">home_cms_block_5</argument>
                </arguments>
            </block>
            <block class="Magento\Cms\Block\Block" name="homeblock6">
                <arguments>
                    <argument name="block_id" xsi:type="string">home_cms_block_6</argument>
                </arguments>
            </block>
        </referenceContainer>

        <referenceContainer name="sidebar.additional">
            <block class="Magento\Cms\Block\Block" name="home_followme">
                <arguments>
                    <argument name="block_id" xsi:type="string">home_follow_me</argument>
                </arguments>
            </block>
            <block class="Magento\Cms\Block\Block" name="home_profile">
                <arguments>
                    <argument name="block_id" xsi:type="string">profile_sidebar</argument>
                </arguments>
            </block>
        </referenceContainer>

        <referenceBlock name="catalog.compare.sidebar" remove="true" />
        <referenceBlock name="wishlist_sidebar" remove="true" />

    </body>
</page>